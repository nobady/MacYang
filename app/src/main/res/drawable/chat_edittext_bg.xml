<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <stroke android:width="@dimen/line_1px" />
            <corners android:radius="6dp" />
        </shape>
    </item>
    <item
        android:bottom="1dp"
        android:left="1dp"
        android:right="1dp"
        android:top="1dp">
        <shape>
            <stroke
                android:width="@dimen/line_1px"
                android:color="@color/colorPrimary" />
            <corners android:radius="6dp" />
        </shape>
    </item>
</layer-list>